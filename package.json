  "engines": {
    "node": ">=6.0.0",
    "npm": ">=5.3.0"
  },
  "main": "index.html",
  "scripts": {
    "build:dist-cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist-cjs",
    "build:dist-esm": "cross-env BABEL_ENV=esm babel src --out-dir dist-esm",
    "build": "npm run clean && npm run build:dist-cjs && npm run build:dist-esm",
    "clean": "rimraf dist-*",
    "format": "prettier --write \"**/*.{js,json,md}\" \".*rc\"",
    "deploy": "now deploy dist --static --public"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-live": "^1.9.2",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-styled-components": "^1.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "glamor": "^2.20.40",
    "parcel-bundler": "^1.6.1",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "serve": "^6.4.10"
  }
}
